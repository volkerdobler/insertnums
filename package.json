{
	"name": "insertnums",
	"version": "0.6.1",
	"publisher": "volkerdobler",
	"displayName": "Insert Nums",
	"description": "Insert Numbers or Alphacharts to VSCode textdocuments",
	"keywords": [
		"sequences",
		"numbers",
		"insert",
		"alphachars"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/volkerdobler/insertnums/issues",
		"email": "vscode.insertnums.ext@volker-dobler.de"
	},
	"homepage": "https://github.com/volkerdobler/insertnums/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/volkerdobler/insertnums.git"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.insertNums",
		"onCommand:extension.insertNums.showHistory"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "insertnums",
			"properties": {
				"insertnums.historyLimit": {
					"type": "number",
					"default": 30,
					"description": "Maximum number of historical records, 0 is unlimited"
				},
				"insertnums.editHistory": {
					"type": "boolean",
					"default": false,
					"description": "Picked item from PickHistory is editable (default: false)"
				}
			}
		},
		"commands": [
			{
				"command": "extension.insertNums",
				"title": "Insert Numbers"
			},
			{
				"command": "extension.insertNums.showHistory",
				"title": "Insert Numbers Show History"
			}
		],
		"keybindings": [
			{
				"command": "extension.insertNums",
				"key": "ctrl+alt+.",
				"mac": "cmd+alt+.",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.insertNums.showHistory",
				"key": "ctrl+alt+,",
				"mac": "cmd+alt+,",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.15",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.2.0",
		"typescript": "^3.9.7",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"@types/d3-format": "^1.3.1",
		"d3-format": "^1.4.4"
	},
	"icon": "insertnums.png",
	"galleryBanner": {
		"color": "#5757ff",
		"theme": "dark"
	}
}
